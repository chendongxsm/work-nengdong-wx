/**app.wxss**/
page{
  width: 100%;
  height: 100%;
}
.container {
  min-height: 100%;
  box-sizing: border-box;
  color: #333;
  background: #f5fffe;
} 

/*表单样式*/
.form-wrapper{
  width: 466rpx;
  margin: 0 auto;
  padding-top: 60rpx;
}
.form-wrapper .icon-input-group{
  display: flex;
  align-items: center;
  width: 100%;
  height: 80rpx;
  margin-bottom: 30rpx;
  padding: 24rpx 0;
  border: 1px solid #00998d;
  border-radius: 4rpx;
  box-sizing: border-box;
}
.form-wrapper .icon-input-group>image{
  flex: none;
  width: 32rpx;
  height: 32rpx;
  padding: 2rpx 14rpx;
  border-right: 1px solid #00998d;
}
.form-wrapper .icon-input-group>input{
  flex: auto;
  padding: 0 14rpx;
  font-size: 24rpx;
}
.form-wrapper .form-button button{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 36rpx;
  color: #fff;
  background: #00998d;
  border-radius: 4rpx;
}
.form-wrapper .form-operate{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  margin-top: 15rpx;
  font-size: 24rpx;
  color: #999;
}
.form-wrapper .icon-input-group picker{
  position: relative;
  width: 100%;
  padding-left: 14rpx;
  font-size: 24rpx;
}
.form-wrapper .icon-input-group picker image{
  position: absolute;
  top: 0;
  right: 14rpx;
  width: 34rpx;
  height: 34rpx;
}
.form-wrapper .form-tips{
  margin-top: -10rpx;
  margin-bottom: 30rpx;
  font-size: 24rpx;
  color: #999;
  white-space:nowrap;
}
.form-wrapper .form-tips text{
  color: #fe2b2b;
}
.placeholder{
  color: #787878;
}
.form-wrapper .icon-input-group .icon-input-group-suffix{
  width: 148rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 24rpx;
  color: #fff;
  background: #00998d;
}
/*弹框样式*/
.dialog-wrapper{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.35);
}
.dialog{
  position: relative;
  width: 468rpx;
  height: 468rpx;
  margin-top: -120rpx;
  line-height: 45rpx;
  text-align: center;
  font-size: 30rpx;
  background: #fff;
  border-radius: 20rpx;
}
.dialog-btn-wrapper{
  position: absolute;
  left: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  width: 100%;
  height: 70rpx;
  border-top: 1px solid #eee;
}
.dialog-btn-wrapper .btn{
  width: 50%;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
}
.dialog-btn-wrapper .btn-gray{
  color: #999;
}
.dialog-btn-wrapper .btn-blue{
  color: #0e90f5;
}
.dialog-btn-wrapper .btn:last-child{
  border-left: 1px solid #eee;
}
.dialog .tips{
  font-size: 24rpx;
  color: #999;
}